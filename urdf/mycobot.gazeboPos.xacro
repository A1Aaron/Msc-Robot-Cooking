<?xml version="1.0" ?>
<robot name="mycobot" xmlns:xacro="http://www.ros.org/wiki/xacro">

<xacro:include filename="$(find mycobot)/urdf/materials.xacro" />
<xacro:include filename="$(find mycobot)/urdf/transmissionPos.xacro" />
<xacro:include filename="$(find mycobot)/urdf/gazebo.xacro" />
<xacro:property name="effort" value="10.0"/>
<xacro:property name="velocity" value="0.5"/>
<xacro:property name="friction" value="0.5"/>
<xacro:property name="damping" value="0.03"/>


<xacro:macro name="mimic_joint_plugin_gazebo" params="name_prefix parent_joint mimic_joint has_pid:=true multiplier:=1.0 offset:=0 sensitiveness:=0.0 max_effort:=1.0 robot_namespace:='/mycobot1'">
  <gazebo>
    <plugin name="${name_prefix}mimic_joint_plugin" filename="libroboticsgroup_upatras_gazebo_mimic_joint_plugin.so">
      <joint>${parent_joint}</joint>
      <mimicJoint>${mimic_joint}</mimicJoint>
      <xacro:if value="${has_pid}">                     <!-- if set to true, PID parameters from "/gazebo_ros_control/pid_gains/${mimic_joint}" are loaded -->
        <hasPID />
      </xacro:if>
      <multiplier>${multiplier}</multiplier>
      <offset>${offset}</offset>
      <sensitiveness>${sensitiveness}</sensitiveness>   <!-- if absolute difference between setpoint and process value is below this threshold, do nothing; 0.0 = disable [rad] -->
      <maxEffort>${max_effort}</maxEffort>              <!-- only taken into account if has_pid:=true [Nm] -->
      <xacro:unless value="${robot_namespace == '/mycobot1'}">
        <robotNamespace>${robot_namespace}</robotNamespace>
      </xacro:unless>
    </plugin>
  </gazebo>
</xacro:macro>

